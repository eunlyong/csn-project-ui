<csn-conf>
  <db>
    <mysql-url>jdbc:mysql://localhost:3306/csn</mysql-url>
    <mysql-user>root</mysql-user>
    <mysql-password>root</mysql-password>
  </db>
  <persistentDB>
    <mongo-url>localhost</mongo-url>
    <mongo-port>27017</mongo-port>
    <mongo-db-name>CSN</mongo-db-name>
  </persistentDB>
  <adapters>
  	<socket>
  		<port>55555</port>
  	</socket>
  </adapters>
</csn-conf>